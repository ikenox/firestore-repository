    "testcontainers": "^10.14.0",
    "typescript": "^5.7.2",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^2.1.6"
  },
  "packageManager": "pnpm@9.15.0",
  "engines": {
    "node": "22.11.0"
  }
}
